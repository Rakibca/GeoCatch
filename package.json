{
  "name": "geocatch",
  "version": "1.0.0",
  "description": "Location-based photography game",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/seeds/seeds.js && node server/server.js",
    "develop": "concurrently \"cd server && node server.js\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdickson96/GeoCatch.git"
  },
  "keywords": [
    "MERN",
    "React",
    "Express",
    "MongoDB",
    "Mongoose",
    "Node",
    "Apollo",
    "Stripe",
    "GraphQL",
    "Full-stack"
  ],
  "author": "Nathan Howes, Rakibul Islam, Trent Dickson",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "exif-js": "^2.3.0"
  }
}
